<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="waldo_sushi.form.sushi_type" class="Waldo\SushiBundle\Form\Type\SushiType">
            <tag name="form.type" alias="sushi" />
        </service>
        
        <service id="waldo_sushi.form.utilisateur_type" class="Waldo\SushiBundle\Form\Type\UtilisateurType">
            <tag name="form.type" alias="utilisateur" />
        </service>

        <service id="waldo_sushi.form.utilisateur_password_type" class="Waldo\SushiBundle\Form\Type\MotDePasseType">
            <tag name="form.type" alias="utilisateur_password" />
        </service>

    </services>
</container>
